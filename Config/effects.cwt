alias[effect:play_song] = scalar

## scope = country
alias[effect:scoped_play_song] = scalar


#if updating this, update every_country_with_original_tag
single_alias[every_effect_clause] = {
	### Apply the effect to this number of randomly selected entities
	## cardinality = 0..1
	random_select_amount = int[1..inf]
	## cardinality = 0..1
	display_individual_scopes = bool
	## cardinality = 0..1
	visible_when_empty = bool
	## cardinality = 0..1
	limit = {
		alias_name[trigger] = alias_match_left[trigger]
	}
    ## cardinality = 0..1
    tooltip = localisation
    ## cardinality = 1..inf
    alias_name[effect] = alias_match_left[effect]
}

#if updating this, update random_country_with_original_tag
single_alias[random_effect_clause] = {
	## cardinality = 0..1
	limit = {
		alias_name[trigger] = alias_match_left[trigger]
	}
	## cardinality = 0..1
	visible_when_empty = bool
	## cardinality = 0..1
	tooltip = localisation
	alias_name[effect] = alias_match_left[effect]
}

single_alias[random_prio_state_effect_clause] = {
	## cardinality = 0..1
	limit = {
		alias_name[trigger] = alias_match_left[trigger]
	}
	## cardinality = 0..1
	prioritize = {
		## cardinality = ~1..inf
		<state>
	}
	## cardinality = 0..1
	visible_when_empty = bool
	## cardinality = 0..1
	tooltip = localisation
	alias_name[effect] = alias_match_left[effect]
}