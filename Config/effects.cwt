### Effect not shown in tooltips.
## scope = any
alias[effect:hidden_effect] = {
	alias_name[effect] = alias_match_left[effect]
}

### Adds legitimacy to scope country. Value has to be 0-100.
## scope = country
alias[effect:add_legitimacy] = int_variable_field

### Kills a ideology leader and removes him completely, making the next in line the new party leader.
## scope = country
alias[effect:retire_ideology_leader] = <ideology>

### Kills a ideology leader and removes him completely, making the next in line the new party leader.
## scope = country
alias[effect:retire_ideology_leader] = variable_field

