types = {
	type[country_history] = {
		path = "game/history/countries"
        type_per_file = yes
	}
}

country_history = {
	capital = <state>

	## cardinality = 0..10
	oob = <oob>
	alias_name[effect] = alias_match_left[effect]
	## cardinality = 0..1
	set_convoys = int
}

enums = {
	complex_enum[unit_leader_ids] = {
		path = "game/history/countries"
		name = {
			id = enum_name
		}
	}
}
